# ========================================
# JCB-OCR Environment Template
# Copy this file to .env and fill in your actual values
# ========================================

# ========================================
# Qwen-VL Configuration
# ========================================
# Get your API key from: https://dashscope.aliyun.com/
QWEN_API_KEY=sk-your-qwen-api-key
QWEN_SERVER_URL=https://your-qwen-server-url
QWEN_MODEL_NAME=Qwen/Qwen3-VL-32B-Instruct
# QWEN_TIMEOUT=600  # optional override

# ========================================
# DeepSeek Configuration
# ========================================
# Get your API key from: https://platform.deepseek.com/
DEEPSEEK_API_KEY=sk-your-deepseek-api-key
DEEPSEEK_SERVER_URL=https://your-deepseek-server-url
DEEPSEEK_MODEL_NAME=deepseek-ai/DeepSeek-OCR
# DEEPSEEK_TIMEOUT=600

# ========================================
# Hunyuan Configuration
# ========================================
# Get your API key from: https://cloud.tencent.com/product/hunyuan
HUNYUAN_API_KEY=your-hunyuan-api-key
HUNYUAN_SERVER_URL=http://your-hunyuan-server-url
HUNYUAN_MODEL_NAME=Tencent-Hunyuan/HunyuanOCR
# HUNYUAN_TIMEOUT=600

# ========================================
# Common Configuration (Optional)
# ========================================
DEFAULT_CLIENT_TYPE=qwen
DEFAULT_TIMEOUT=600
DEFAULT_MAX_RETRIES=3
DEFAULT_TEMPERATURE=0.0
DEFAULT_TOP_P=0.01

# ========================================
# Legacy/Fallback Configuration
# ========================================
# Used when --server-url is not specified
MINERU_VL_SERVER=http://your-mineru-server-url


